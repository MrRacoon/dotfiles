let mapleader="\"
map <Space> <Leader>

inoremap kj <escape>

set hintchars=asdflkjweroiucvmnbt

nnoremap <Leader>vr :source ~/.vimperatorrc<cr>
nnoremap <Leader>ee :echo "woot"<cr>

nnoremap <Leader>tt :tabdetach<cr>

nnoremap ; :

nnoremap s o
nnoremap S t

nnoremap J 10j
nnoremap K 10k

nnoremap h :tabprevious<cr>
nnoremap l :tabnext<cr>
